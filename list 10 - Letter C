using System;

namespace PATrilhasAtiv07
{
	class Program
	{
		public static void Main(string[] args)
		{
			int[,] Matrix = {{2,4}, {4,5}, {2,1}, {3,6}};
			
			int soma = 0;
			
			for(int i = 0; i<4; i++)
			{
				for(int j =0; j<2; j++)
				{
					soma += Matrix[i,j];
				}
				
			}
			Console.WriteLine(soma);
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
